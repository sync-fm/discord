# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=1429085779367428219
# Allow regular youtube.com/youtu.be links instead of just music.youtube.com (defaults to false) (as syncfm doesnt support it, but might add in the future)
DISCORD_ENABLE_YOUTUBE=false

# Analytics (optional)
# PostHog project API key enables server-side analytics capture.
POSTHOG_API_KEY=
# POSTHOG_HOST defaults to https://eu.i.posthog.com when omitted.
POSTHOG_HOST=
# Set to true to explicitly disable analytics even if API key is present.
POSTHOG_DISABLED=false
# Fine-tune batching behaviour (optional).
POSTHOG_FLUSH_AT=1
POSTHOG_FLUSH_INTERVAL_MS=1000

